{
  "ci": {
    "collect": {
      "url": ["http://localhost:3000/"],
      "startServerCommand": "yarn build && yarn start",
      "numberOfRuns": 3
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", {"minScore": 0.9}],
        "categories:accessibility": ["error", {"minScore": 1}]
      }
    }
  }
}
