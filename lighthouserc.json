{
    "ci": {
      "collect": {
        "startServerCommand": "yarn build && yarn start",
        "url": ["http://localhost:34331/"],
        "numberOfRuns": 3
      },
      "upload": {
        "target": "temporary-public-storage"
      },
      "assert": {
        "assertions": {
          "performance-budget": ["error", {"minScore":50}]
        }
      }
    }
}